{
    "compilerOptions": {
        "outDir": "./dist",
        "target": "es5",
        "module": "es6",
        "jsx": "react",
        "noImplicitAny": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "plugins": [{ "name": "typescript-plugin-css-modules" }],
        "paths": {
            "@mui/styled-engine": ["./node_modules/@mui/styled-engine-sc"]
        }
    }
}
